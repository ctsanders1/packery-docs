<div class="in-use-gallery">
  {{#each in-use}}
    <div class="in-use-gallery__item">
      <a class="in-use-gallery__item__link" href="{{url}}">
        <p class="in-use-gallery__item__title">{{{title}}}</p>
        <img class="in-use-gallery__item__image" src="{{image}}" />
      </a>
    </div>
  {{/each}}
</div>
